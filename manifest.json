{
    "manifest_version": 3,
    "name": "Redirect to Supercast",
    "version": "1.0",
    "description": "Automatically redirects from Warpcast to Supercast.",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
        "*://*.warpcast.com/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}