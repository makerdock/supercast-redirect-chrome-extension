{
    "manifest_version": 3,
    "name": "Redirect to Supercast",
    "version": "1.0",
    "description": "Automatically redirects from Warpcast to Supercast.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.warpcast.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}